game.Players.LocalPlayer.Chatted:connect(function(msg)
if msg:lower():sub(1,3) == "ff " then
if (msg:sub(4)) == "me" then
game.Players.LocalPlayer.Character.Humanoid.MaxHealth = math.huge
FF = Instance.new("Part",game.Players.LocalPlayer.Character)
FF.Anchored = true
FF.Archivable = false
FF.Locked = true
FF.Transparency = 0.5
FF.BrickColor = BrickColor.new("Really black")
FF.TopSurface = Enum.SurfaceType.Smooth
FF.BottomSurface = Enum.SurfaceType.Smooth
FF.CanCollide = false
FF.Shape = "Ball"
FF.Touched:connect(function(Hit)
if Hit.Parent then
if Hit.Parent.Name ~= game.Players.LocalPlayer.Name then
if Hit.Parent:FindFirstChild("Humanoid") then
		BF = Instance.new("BodyForce",Hit.Parent.Torso)
		Shock = coroutine.wrap(function()
		while true do
		Hit.Parent.Torso.CFrame = Hit.Parent.Torso.CFrame * CFrame.Angles(0, math.pi, 0)
		wait()
		end
		end)
		Shock()
		BF.force = Vector3.new(0, 800, 0)
		wait(1)
		BF.force = Vector3.new(0, 100, 0)
		wait(3)
		BF.force = Vector3.new(0, -600000, 0)
		
		local chatgui = Instance.new("BillboardGui")
		chatgui.Parent = Hit.Parent.Head
chatgui.Adornee = Hit.Parent.Head
chatgui.Size = UDim2.new(1, 0, 1, 0)
chatgui.StudsOffset = Vector3.new(0, 2, 0)

local text = Instance.new("TextLabel")
text.Parent = chatgui
text.Transparency = 1
text.Position = UDim2.new(-9.5, 0, -1.5, 0)
text.Size = UDim2.new(20, 0, 3, 0)
text.FontSize = "Size18"
text.TextColor3 = Color3.new(0, 0, 0)
text.TextStrokeColor3 = Color3.new(1, 1, 1)
text.TextStrokeTransparency = 0
text.Text = "Shocking, isn't it?"
text.TextWrapped = true
text.TextScaled = false
text.TextTransparency=0
for i = 1,40 do
text.Position = text.Position - UDim2.new(0, 0, 0.025, 0)
wait()
end
for i = 1,40 do
text.TextTransparency = text.TextTransparency + 0.025
text.TextStrokeTransparency = text.TextStrokeTransparency + 0.025
wait()
end
wait(1)
text:Destroy()
chatgui:Destroy()
end
end
end
end)
