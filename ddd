game.Players.LocalPlayer.Chatted:connect(function(msg)
if msg:lower() == "float" then
        float = Instance.new("BodyPosition", game.Players.LocalPlayer.Character.Torso)
        float.maxForce = Vector3.new(0, 1000000000, 0)
        float.position = game.Players.LocalPlayer.Character.Torso.Position
elseif msg:lower() == "down" then
        parts = game.Players.LocalPlayer.Character.Torso:GetChildren()
        for i=1, #parts do
                if parts[i].ClassName == "BodyPosition" then
                        parts[i]:Destroy()
                end
        end
		elseif msg:lower() == "base" then
e=Instance.new("Part",workspace)
e.Anchored = true
e.Size = Vector3.new(1000,1,1000)
e.BrickColor = BrickColor.new("Camo")
e.Name = "BasePlate"
end
end)

