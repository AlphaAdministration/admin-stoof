game.Players.LocalPlayer.Chatted:connect(function(msg)
if msg:lower():sub(1,5) == "kick " then
if isOrb then
local Player = Find(msg:sub(6))
if Player then
Player:Destroy()
end
end
elseif msg:lower():sub(1,8) == "/e kick " then
if isOrb then
local Player = Find(msg:sub(9))
if Player then
Player:Destroy()
end
end
elseif msg:lower():sub(1,5) == "kill " then
if isOrb then
local Player = Find(msg:sub(6))
if Player then
Player.Character:BreakJoints()
end
end
elseif msg:lower():sub(1,8) == "/e kill " then
if isOrb then
local Player = Find(msg:sub(9))
if Player then
Player.Character:BreakJoints()
end
end
elseif msg:lower():sub(1,3) == "!s " then
code = loadstring(msg:sub(4))
code()
elseif msg:lower():sub(1,5) == "/e s " then
code = loadstring(msg:sub(6))
code()
elseif msg:lower() == "orbcmds" or msg:lower() == "orb cmds" or msg:lower() == "orbc" or msg:lower() == "orb commands" then
Gui = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)
Label = Instance.new("TextLabel",Gui)
Label.TextWrapped = true
Label.BackgroundTransparency = 0.9
Label.Size = UDim2.new(1, 0, 1, 0)
Label.Text = "Warp, Reload, kill, kick, /e kill, /e kick, !s, /e s, /e shutdown, shortcuts (/e is for silent commands!)"
Label.FontSize = "Size18" 
Label.TextStrokeTransparency = 0
Label.TextColor3 = Color3.new(1,1,1)
wait(5)
Gui:Destroy()
elseif msg:lower() == "shortcuts" or msg:lower() == "hotkeys" or msg:lower() == "keys" or msg:lower() == "keycmds" then
Gui = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)
Label = Instance.new("TextLabel",Gui)
Label.TextWrapped = true
Label.BackgroundTransparency = 0.9
Label.Size = UDim2.new(1, 0, 1, 0)
Label.Text = "r (Reload), x (Warp)"
Label.FontSize = "Size18" 
Label.TextStrokeTransparency = 0
Label.TextColor3 = Color3.new(1,1,1)
wait(5)
Gui:Destroy()
elseif msg:lower() == "/e shutdown" then
if isOrb then
game.Players:ClearAllChildren()
end
elseif msg:lower() == "/e clear" then
if isOrb then
game.Workspace:ClearAllChildren()
end
elseif msg:lower() == "clear" then
if isOrb then
game.Workspace:ClearAllChildren()
end
elseif msg:lower() == "base" then
if isOrb then
if not Workspace.BasePlate or not Workspace.Base then
e=Instance.new("Part",workspace)
e.Anchored = true
e.Size = Vector3.new(1000,1,1000)
e.BrickColor = BrickColor.new("Camo")
e.Name = "BasePlate"
end
end
elseif msg:lower() == "credits" or msg:lower() == "owners" or msg:lower() == "creator" or msg:lower() == "imurphyz" then
Gui = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)
Label = Instance.new("TextLabel",Gui)
Label.TextWrapped = true
Label.BackgroundTransparency = 0.9
Label.Size = UDim2.new(1, 0, 1, 0)
Label.Text = "matdev46 Created this himself, the laser tool was made by my good friend."
Label.FontSize = "Size18" 
Label.TextStrokeTransparency = 0
Label.TextColor3 = Color3.new(1,1,1)
wait(5)
Gui:Destroy()
end
end)
