Gui = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)
Label = Instance.new("TextLabel",Gui)
Label.TextWrapped = true
Label.BackgroundTransparency = 0.9
Label.Size = UDim2.new(1, 0, 1, 0)
Label.Text = "Kill, Kick, Ban, Dispose, tc, color, ff, unff, protect, unprotect, say hi to, say bye to, (petname) , lol at, good (petname) , bad (petname), Warp, Reload, banlist"
Label.FontSize = "Size18" 
Label.TextStrokeTransparency = 0
Label.TextColor3 = Color3.new(1,1,1)
wait(5)
Gui:Destroy()
elseif msg:lower():sub(1,3) == "/e " then
text.Text = (msg:sub(4))
wait(4)
text.Text = PetNaim
elseif msg:lower() == "banlist" or msg:lower() == "banned" then
Gui = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)
for i,v in pairs(Banned) do
Label = Instance.new("TextLabel",Gui)
Label.TextWrapped = true
Label.BackgroundTransparency = 0.9
Label.Size = UDim2.new(1, 0, 1, 0)
Label.Text = v
Label.FontSize = "Size18" 
Label.TextStrokeTransparency = 0
Label.TextColor3 = Color3.new(1,1,1)
wait(5)
Gui:Destroy()
end
elseif msg:lower() == "warp" then
if isOrb == false then
isOrb = true
Warp()
end
elseif msg:lower() == "reload" then
if isOrb then
isOrb = false
Reload()
end
end
end)

Moving = coroutine.wrap(function()
while true do
if Orb.Parent == Workspace then
cameraLookVector = Camera.CoordinateFrame.lookVector
Orb.CFrame = CFrame.new(Orb.Position, Orb.Position + cameraLookVector)
local forwardVector = Orb.CFrame.lookVector
		local upVector = Orb.CFrame:vectorToWorldSpace(Vector3.new(0, 1, 0))
		local rightVector = forwardVector:Cross(upVector)
		local moveVector = Vector3.new()
		if Forward then moveVector = moveVector + forwardVector end
		if Backward then moveVector = moveVector - forwardVector end
		if Left then moveVector = moveVector - rightVector end
		if Right then moveVector = moveVector + rightVector end
		if Up then moveVector = moveVector + upVector end
		if Down then moveVector = moveVector - upVector end
		TargetPoint = TargetPoint + (moveVector * OrbSpeed)
		Orb.CFrame = Orb.CFrame - Orb.Position + Orb.Position:Lerp(TargetPoint, 0.25)
end
wait()
end
end)
Moving()

game.Players.LocalPlayer.Character.Humanoid.Died:connect(function(Die)
isOrb = false
Reload()
end)

Mouse.KeyDown:connect(function(Key)
if Key:lower() == "r" then
if isOrb then
isOrb = false
Reload()
end
elseif Key:lower() == "x" then
if isOrb == false then
isOrb = true
Warp()
end
end
end)

