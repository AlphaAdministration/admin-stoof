Mouse.KeyDown:connect(function(key) OnKey(key, true) end)
Mouse.KeyUp:connect(function(key) OnKey(key, false) end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
if msg:lower() == "load" then
p=Instance.new("Part",game.Players.LocalPlayer.Character)
p.Anchored = true
p.Name = PetNaim
p.Transparency = 0
p.Size = Vector3.new(3,3,3)
p.Material = "Slate"
p.BrickColor = BrickColor.new("Black")
p.Shape = "Ball"
local chatgui = Instance.new("BillboardGui")
chatgui.Parent = p
chatgui.Adornee = p
chatgui.Size = UDim2.new(1, 0, 1, 0)
chatgui.StudsOffset = Vector3.new(0, 2, 0)

text = Instance.new("TextLabel")
text.Parent = chatgui
text.Transparency = 1
text.Position = UDim2.new(-9.5, 0, -1.5, 0)
text.Size = UDim2.new(20, 0, 3, 0)
text.FontSize = "Size18"
text.TextColor3 = Color3.new(0, 0, 0)
text.TextStrokeColor3 = Color3.new(1, 1, 1)
text.TextStrokeTransparency = 0
text.Text = PetNaim
text.TextWrapped = true
text.TextScaled = false
text.TextTransparency=0
PetLoop = coroutine.wrap(function()
while true do
p.Position = game.Players.LocalPlayer.Character.Torso.Position + Vector3.new(0, 5, 7)
p.CFrame = p.CFrame * CFrame.Angles(math.pi/400, math.pi/400, math.pi/400)
wait()
p.CFrame = p.CFrame * CFrame.Angles(math.pi, math.pi, math.pi)
end
end)
PetLoop() 
elseif msg:lower():sub(1, #PetNaim) == PetNaim:lower() then
JeromyMsg = "Yes Master?"
for i = 1,#JeromyMsg do
	text.Text = string.sub(JeromyMsg, 1, i)
	wait(0.05)
end
wait(3)
text.Text = PetNaim
elseif msg:lower() == "stop" then
p:Destroy()
elseif msg:lower():sub(1,8) == "protect " then
if (msg:sub(9)) == "me" then
text.Text = "Master, you are now protected!"
game.Players.LocalPlayer.Character.Humanoid.MaxHealth = math.huge
wait(3)
text.Text = PetNaim
else
local Player = Find(msg:sub(9))
if Player then
local Z = Instance.new("Part") 
Z.Name = "Shoot" 
local Place0 = game.Players.LocalPlayer.Character[PetNaim].CFrame
Z.FormFactor = Enum.FormFactor.Custom
Z.TopSurface = Enum.SurfaceType.Smooth
Z.BottomSurface = Enum.SurfaceType.Smooth
Z.Size = Vector3.new(0.4,0.4,(Place0.p - Player.Character.Torso.Position).magnitude) 
Z.CFrame = CFrame.new((Place0.p + Player.Character.Torso.Position)/2,Place0.p) 
Z.Parent = game.Players.LocalPlayer.Character
Z.BrickColor = BrickColor.new("White")
Z.Transparency = 0.5 
Z.Reflectance = 0.5 
Z.Anchored = true
Z.CanCollide = false 
M = Instance.new("BlockMesh",Z)
M.Scale = Vector3.new(0.5,0.5,1)
for i = 1,5 do 
Z.Transparency = 0.25+(i*0.1) 
Z.Reflectance = i*0.1 
wait() 
end 
Z:Destroy()
text.Text = "Protecting "..Player.Name..", Master!"
Player.Character.Humanoid.MaxHealth = math.huge
wait(3)
text.Text = PetNaim
end
end
elseif msg:lower():sub(1,10) == "say hi to " then
if (msg:sub(11)) == "me" then
text.Text = "Hi Master!"
wait(3)
text.Text = PetNaim
else
local Player = Find(msg:sub(11))
if Player then
