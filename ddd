M.Scale = Vector3.new(0.5,0.5,1)
for i = 1,5 do 
Z.Transparency = 0.25+(i*0.1) 
Z.Reflectance = i*0.1 
wait() 
end 
Z:Destroy()
text.Text = "Hi "..Player.Name..", my name is "..PetNaim.."!"
wait(3)
