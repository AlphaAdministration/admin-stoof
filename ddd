tool = Instance.new("HopperBin",game.Players.LocalPlayer.Backpack)
tool.Name = "Laser"

script.Parent = game.Players.LocalPlayer.Backpack.Laser

local Banned = {"Noob","iCoud"}
local Selected = false 
local Shoot = true
Animate = false
Particles = true
Rotate = true
Trail = true
isOrb = false --Don't touch this.
local Left = false
local Right = false
local Up = false
local Down = false
local Forward = false
local Backward = false
local TargetPoint = Vector3.new()
Mouse = game.Players.LocalPlayer:GetMouse()
PetNaim = "Alpha" --PET NAME HERE!!!
OrbSpeed = 2 --Speed
Follow = false
Camera = Workspace.CurrentCamera

Orb = Instance.new("Part",game.Lighting)
Orb.Size = Vector3.new(3,3,3)
Orb.Name = PetNaim
Orb.Anchored = true
Orb.Material = "Slate"
Orb.BrickColor = BrickColor.new("Black")
Orb.Shape = "Ball"
local orbname = Instance.new("BillboardGui")
orbname.Parent = Orb
orbname.Adornee = Orb
orbname.Size = UDim2.new(1, 0, 1, 0)
orbname.StudsOffset = Vector3.new(0, 2, 0)
orbtext = Instance.new("TextLabel")
orbtext.Parent = orbname
orbtext.Transparency = 1
orbtext.Position = UDim2.new(-9.5, 0, -1.5, 0)
orbtext.Size = UDim2.new(20, 0, 3, 0)
orbtext.FontSize = "Size18"
orbtext.TextColor3 = Color3.new(0, 0, 0)
orbtext.TextStrokeColor3 = Color3.new(1, 1, 1)
orbtext.TextStrokeTransparency = 0
orbtext.Text = PetNaim
orbtext.TextWrapped = true
orbtext.TextScaled = false
orbtext.TextTransparency=0
game.Players.LocalPlayer.Chatted:connect(function(Msg)
if (Msg:sub(1,2)) ~= "/e" then
orbtext.Text = Msg
wait(4)
orbtext.Text = PetNaim
end
end)

local function OnKey(key, isDown)
	key = key:lower()
	if key == 'w' or key == '' then
		Forward = isDown
	elseif key == 's' or key == '' then
		Backward = isDown
	elseif key == 'a' or key == '' then
		Left = isDown
	elseif key == 'd' or key == '' then
		Right = isDown
	elseif key == 'e' then
		Up = isDown
	elseif key == 'q' then
		Down = isDown
end
end

function Warp()
Camera.CameraSubject = Orb
Camera.CameraType = Enum.CameraType.Follow
Orb.CFrame = game.Players.LocalPlayer.Character.Torso.CFrame
Orb.Velocity = Vector3.new()
Orb.Parent = Workspace
game.Players.LocalPlayer.Character:MoveTo(Vector3.new(5e6, 5e6, 5e6))
game.Players.LocalPlayer.Character.Torso.Anchored = true
TargetPoint = Orb.Position
end

function Reload()
Orb.Parent = game.Lighting
game.Players.LocalPlayer.Character:MoveTo(Orb.Position)
game.Players.LocalPlayer.Character.Torso.Anchored = false
game.Players.LocalPlayer.Character.Torso.Velocity = Vector3.new()
Camera.CameraSubject = game.Players.LocalPlayer.Character
Camera.CameraType = Enum.CameraType.Custom
end

game.Players.PlayerAdded:connect(function(name)
for i,v in pairs(Banned) do
if name.Name:lower() == v.Name:lower() then
v:Destroy()
end
end
end)

function Find(name)
for i,v in ipairs(game.Players:GetPlayers()) do
if v.Name:sub(1,#name):lower()== name:lower()then
return v
end
end
end

function Click(Target) 
		local P = Instance.new("Part") 
		P.Name = "pewpew" 
		local Place0 = script.Parent.Parent.Parent.Character["Left Arm"].CFrame 
		P.FormFactor = Enum.FormFactor.Custom
		P.TopSurface = Enum.SurfaceType.Smooth
		P.BottomSurface = Enum.SurfaceType.Smooth
		P.Size = Vector3.new(0.4,0.4,(Place0.p - Target.Hit.p).magnitude) 
		P.CFrame = CFrame.new((Place0.p + Target.Hit.p)/2,Place0.p) 
		P.Parent = game.Workspace 
		P.Color = Color3.new(0,0,1) 
		P.Transparency = 0.5 
		P.Reflectance = 0.5 
		P.Anchored = true 
		P.CanCollide = false 
		M = Instance.new("BlockMesh",P)
		M.Scale = Vector3.new(0.5,0.5,1)
		Laser = Instance.new("Sound")
		Laser.SoundId = "http://www.roblox.com/asset/?id=132594796"
		Laser.Parent = game.SoundService
		Laser.Volume = .7
		Laser:Play()
		wait()
		Laser:Destroy()
		for i = 1,5 do 
			P.Transparency = 0.25+(i*0.1) 
			P.Reflectance = i*0.1 
			wait() 
			end 
		P:Destroy()
		Target.Target.Parent:findFirstChild("Humanoid").Health = 0
		if Target.Target.Parent:findFirstChild("Humanoid").Health == 0 then
		local chatgui = Instance.new("BillboardGui")
		chatgui.Parent = Target.Target.Parent
chatgui.Adornee = Target.Target.Parent.Head
chatgui.Size = UDim2.new(1, 0, 1, 0)
chatgui.StudsOffset = Vector3.new(0, 2, 0)

local text = Instance.new("TextLabel")
text.Parent = chatgui
text.Transparency = 1
text.Position = UDim2.new(-9.5, 0, -1.5, 0)
text.Size = UDim2.new(20, 0, 3, 0)
text.FontSize = "Size18"
text.TextColor3 = Color3.new(0, 0, 0)
text.TextStrokeColor3 = Color3.new(1, 1, 1)
text.TextStrokeTransparency = 0
text.Text = "Death is apon you..."
text.TextWrapped = true
text.TextScaled = false
text.TextTransparency=0
for i = 1,40 do
text.Position = text.Position - UDim2.new(0, 0, 0.025, 0)
wait()
end
for i = 1,40 do
text.TextTransparency = text.TextTransparency + 0.025
text.TextStrokeTransparency = text.TextStrokeTransparency + 0.025
wait()
end
wait(1)
text:Destroy()
chatgui:Destroy()
end
end

function Select(Mouse) 
	local Arm = script.Parent.Parent.Parent.Character:findFirstChild("Left Arm") 
	if(Arm==nil)then 
		script.Parent:Destroy()
		script:Destroy()
	end 
	Selected = true 
	local Torso = script.Parent.Parent.Parent.Character:findFirstChild("Torso") 
	if(Torso==nil)then 
		script.Parent:Destroy()
		script:Destroy()
	end 
	local ArmWeld = Torso:findFirstChild("Left Shoulder") 
	if(ArmWeld~=nil)then 
		ArmWeld.Parent = nil 
	end 
	
	Mouse.Button1Down:connect(function()Click(Mouse)end) 
	Arm.Anchored = true 
	while Selected do 
		local Place0 = script.Parent.Parent.Parent.Character.Torso.CFrame 
		Place0 = Place0 + ((Place0 * CFrame.fromEulerAnglesXYZ(math.pi/2,0,0)).lookVector * 0.5) + (Place0 * CFrame.fromEulerAnglesXYZ(0,math.pi/2,0)).lookVector 
		local Place1 = Place0 + ((Place0.p-Mouse.Hit.p).unit * -2) 
		Arm.CFrame = CFrame.new((Place0.p + Place1.p)/2,Place0.p) * CFrame.fromEulerAnglesXYZ(-math.pi/2,0,0) 
		wait() 
	end 
	Arm.Anchored = false 
	if(ArmWeld~=nil)then 
		ArmWeld.Parent = Torso 
	end 
end

function Deselect() 
	Selected = false 
end

script.Parent.Selected:connect(Select) 
script.Parent.Deselected:connect(Deselect)

Animation = coroutine.wrap(function()
wait()
lp = game.Players.LocalPlayer
CP,Torso,g=nil,nil,nil
necko=CFrame.new(0, 1, 0, -1, -0, -0, 0, 0, 1, 0, 1, 0)
getPos=function(Pos,TorsoPos)
	return Vector3.new(Pos.x,TorsoPos.y,Pos.z)
end
Turn=function(mouse,Torso,Gyro)
	g.cframe=CFrame.new(Torso.Position,getPos(mouse.Hit.p,Torso.Position))
	offset=(Torso.Position.y-mouse.Hit.p.y)/100
	mag=(Torso.Position-mouse.Hit.p).magnitude/80
	offset=offset/mag
	neck=Torso.Neck
	neck.C0=necko*CFrame.fromEulerAnglesXYZ(offset,0,0)
	arm=Torso:FindFirstChild("Right Shoulder")
	if arm~=nil then
		arm.C0=CFrame.new(1, 0.5, 0, 0, 0, 1, -6.48200512e-007, 1.00000274, 0, -1.00000274, -6.48200512e-007, 0)*CFrame.fromEulerAnglesXYZ(0,0,-offset)
	end
end
Equi=function(mouse)
	wait(.2)	
	CP=lp.Character
	Torso=CP.Torso
	g=Instance.new("BodyGyro")
	g.P=18000
	g.D=600
	g.maxTorque=Vector3.new(80000,80000000,80000)*50000000000000000000000000
	g.cframe=Torso.CFrame
	g.Parent=Torso
	while true do
		Turn(mouse,Torso,g)
		wait()
	end
	Torso.Neck.C0=necko
	g:Remove()
end
Equi(game.Players.LocalPlayer:GetMouse())
end)

if (Animate == true) then
Animation()
end

game.Players.LocalPlayer.Chatted:connect(function(Msg)
if (Msg:sub(1,2)) ~= "/e" then
local chatgui = Instance.new("BillboardGui")
chatgui.Parent = game.Players.LocalPlayer.Character
chatgui.Adornee = game.Players.LocalPlayer.Character.Head
chatgui.Size = UDim2.new(1, 0, 1, 0)
chatgui.StudsOffset = Vector3.new(0, 2, 0)

local text = Instance.new("TextLabel")
text.Parent = chatgui
text.Transparency = 1
text.Position = UDim2.new(-9.5, 0, -1.5, 0)
text.Size = UDim2.new(20, 0, 3, 0)
text.FontSize = "Size18"
text.TextColor3 = Color3.new(0, 0, 0)
text.TextStrokeColor3 = Color3.new(1, 1, 1)
text.TextStrokeTransparency = 0
text.TextTransparency=0
Message = Msg
for i = 1,#Message do
	text.Text = string.sub(Message, 1, i)
	wait(0.05)
end
text.TextWrapped = true
text.TextScaled = false
for i = 1,40 do
text.Position = text.Position - UDim2.new(0, 0, 0.025, 0)
wait()
end
for i = 1,40 do
text.TextTransparency = text.TextTransparency + 0.025
text.TextStrokeTransparency = text.TextStrokeTransparency + 0.025
wait()
end
wait(1)
text:Destroy()
chatgui:Destroy()
end
end)

Particle = coroutine.wrap(function()
while true do
P1 = Instance.new("Part",game.Players.LocalPlayer.Character)
P2 = Instance.new("Part",game.Players.LocalPlayer.Character)
P3 = Instance.new("Part",game.Players.LocalPlayer.Character)
P4 = Instance.new("Part",game.Players.LocalPlayer.Character)
P1.Size = Vector3.new(0.2,0.2,0.2)
P2.Size = Vector3.new(0.2,0.2,0.2)
P3.Size = Vector3.new(0.2,0.2,0.2)
P4.Size = Vector3.new(0.2,0.2,0.2)
P1.BrickColor = BrickColor.new("White")
P2.BrickColor = BrickColor.new("White")
P3.BrickColor = BrickColor.new("Black")
P4.BrickColor = BrickColor.new("Black")
Mesh1 = Instance.new("BlockMesh",P1)
Mesh2 = Instance.new("BlockMesh",P2)
Mesh3 = Instance.new("BlockMesh",P3)
Mesh4 = Instance.new("BlockMesh",P4)
Mesh1.Scale = Vector3.new(0.2,0.2,0.2)
Mesh2.Scale = Vector3.new(0.2,0.2,0.2)
Mesh3.Scale = Vector3.new(0.2,0.2,0.2)
Mesh4.Scale = Vector3.new(0.2,0.2,0.2)
P1.Transparency = 0.5
P2.Transparency = 0.5
P3.Transparency = 0.5
P4.Transparency = 0.5
P1.Anchored = true
P2.Anchored = true
P3.Anchored = true
P4.Anchored = true
P1.CanCollide = false
P2.CanCollide = false
P3.CanCollide = false
P4.CanCollide = false
P1.CFrame = game.Players.LocalPlayer.Character.Head.CFrame * CFrame.new(1,1,4) 
P2.CFrame = game.Players.LocalPlayer.Character.Head.CFrame * CFrame.new(1,-1,-4)
P3.CFrame = game.Players.LocalPlayer.Character.Head.CFrame * CFrame.Angles(0,math.pi,0) * CFrame.new(1,1,4)
P4.CFrame = game.Players.LocalPlayer.Character.Head.CFrame * CFrame.Angles(0,math.pi,0) * CFrame.new(1,-1,-4)
for i = 1,25 do
P1.Transparency = P1.Transparency + 0.05
P2.Transparency = P2.Transparency + 0.05
P3.Transparency = P1.Transparency + 0.05
P4.Transparency = P2.Transparency + 0.05
P1.CFrame = P1.CFrame * CFrame.new(0,0.5,0.3)
P2.CFrame = P2.CFrame * CFrame.new(0,0.5,0.3)
P3.CFrame = P3.CFrame * CFrame.new(0,0.5,0.3)
P4.CFrame = P4.CFrame * CFrame.new(0,0.5,0.3)
wait()
P1.CFrame = P1.CFrame * CFrame.new(0,0.5,-0.1)
P2.CFrame = P2.CFrame * CFrame.new(0,0.5,-0.1)
P3.CFrame = P3.CFrame * CFrame.new(0,0.5,-0.1)
P4.CFrame = P4.CFrame * CFrame.new(0,0.5,-0.1)
wait()
end
wait()
P1:Destroy()
P2:Destroy()
end
end)
Particle()

